{
  "name": "grafserv",
  "version": "0.0.1-alpha.9",
  "description": "A highly optimized server for GraphQL, powered by Grafast",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/servers/node/index.d.ts",
      "default": "./dist/servers/node/index.js"
    },
    "./express/v4": {
      "types": "./dist/servers/express/v4/index.d.ts",
      "default": "./dist/servers/express/v4/index.js"
    },
    "./koa/v2": {
      "types": "./dist/servers/koa/v2/index.d.ts",
      "default": "./dist/servers/koa/v2/index.js"
    },
    "./fastify/v4": {
      "types": "./dist/servers/fastify/v4/index.d.ts",
      "default": "./dist/servers/fastify/v4/index.js"
    }
  },
  "scripts": {
    "prepack": "tsc -b",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphile/heart.git"
  },
  "keywords": [
    "server",
    "grafast",
    "graphql",
    "graphile",
    "fast",
    "graphite"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphile/heart/issues"
  },
  "homepage": "https://github.com/graphile/heart/tree/main/grafast/grafserv",
  "dependencies": {
    "@graphile/lru": "^5.0.0-alpha.2",
    "debug": "^4.1.1",
    "eventemitter3": "^4.0.7",
    "graphile-config": "^0.0.1-alpha.4",
    "graphql-ws": "^5.11.3",
    "ruru": "^2.0.0-alpha.6",
    "tslib": "^2.5.0"
  },
  "engines": {
    "node": ">=16.10"
  },
  "peerDependencies": {
    "grafast": "^0.0.1-alpha.9",
    "graphql": "^16.1.0-experimental-stream-defer.6",
    "ws": "^8.12.1"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^4",
    "@types/koa": "^2",
    "@types/koa-bodyparser": "^4",
    "@whatwg-node/fetch": "^0.8.1",
    "express": "^4.18.2",
    "fastify": "^4.12.0",
    "grafast": "^0.0.1-alpha.9",
    "graphql-http": "^1.16.0",
    "jest": "^28.1.3",
    "jest-serializer-graphql-schema": "^5.0.0-alpha.2",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.3.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist",
    "index.js"
  ]
}